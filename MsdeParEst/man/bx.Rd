## The function is currently defined as
function (x, fixed, random) 
{
    if (sum(random) > 2) {
        return(matrix(c(1, -x), 2, 1))
    }
    if (sum(random) == 1) {
        return(-fixed * x)
    }
    if (sum(random) == 2) {
        return(-x)
    }
    if (sum(random) == 0) {
        return(c(fixed[1], -fixed[2] * x))
    }
  }
}
